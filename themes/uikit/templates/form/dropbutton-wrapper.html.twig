{#
/**
 * @file
 * Theme override to display a dropbutton wrapper.
 *
 * Available variables:
 * - children: Contains the child elements of the dropbutton menu.
 *
 * @see template_preprocess()
 *
 * @ingroup uikit_themeable
 */
#}
{% if children %}
  {% spaceless %}
    {{ children }}
  {% endspaceless %}
{% endif %}
